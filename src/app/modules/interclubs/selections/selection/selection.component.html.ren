<div *ngIf="loading">
    Chargement des donn√©es en cours...
</div>

<div *ngIf="! loading">

    <div *ngIf="selectedInterclubCategoryId === 1 && ldfParticipants && ldfByCategory">
        <app-interclubs-selections-hommes 
            [semaines]=" filteredSemaineByCategory  "
            [listeDesForces]=" filteredListeDesForcesByCategory "
            [teams]="filteredTeamsByCategory"
            [matches]="filteredMatchesByCategory"
            [interclubCategory]="selectedInterclubCategory"
            >
        </app-interclubs-selections-hommes>
    </div>

    <div *ngIf="selectedInterclubCategoryId === 2 && ldfParticipants && ldfByCategory">
        <app-interclubs-selections-dames 
            [semaines]=" filteredSemaineByCategory "
            [listeDesForces]=" filteredListeDesForcesByCategory "
            [teams]="filteredTeamsByCategory"
            [matches]="filteredMatchesByCategory"
            [interclubCategory]="selectedInterclubCategory"
            >
        </app-interclubs-selections-dames>
    </div>

    <div *ngIf="selectedInterclubCategoryId === 3 && ldfParticipants && ldfByCategory">
        <app-interclubs-selections-veterans 
            [semaines]=" filteredSemaineByCategory  "
            [listeDesForces]=" filteredListeDesForcesByCategory "
            [teams]="filteredTeamsByCategory"
            [matches]="filteredMatchesByCategory"
            [interclubCategory]="selectedInterclubCategory"
            >
        </app-interclubs-selections-veterans>
    </div>

    <div *ngIf="selectedInterclubCategoryId === 4 && ldfParticipants && ldfByCategory">
        <app-interclubs-selections-ainees 
            [semaines]=" filteredSemaineByCategory  "
            [listeDesForces]=" filteredListeDesForcesByCategory "
            [teams]="filteredTeamsByCategory"
            [matches]="filteredMatchesByCategory"
            [interclubCategory]="selectedInterclubCategory"
            >
        </app-interclubs-selections-ainees>
    </div>

</div>
